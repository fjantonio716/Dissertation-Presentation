@incollection{SAREVSKI2016227,
title = {Chapter 7 - Water (R718) Turbo Compressor Refrigeration/Heat Pump Systems},
editor = {Milan N. arevski and Vasko N. arevski},
booktitle = {Water (R718) Turbo Compressor and Ejector Refrigeration / Heat Pump Technology},
publisher = {Butterworth-Heinemann},
address = {Boston},
pages = {227-246},
year = {2016},
isbn = {978-0-08-100733-4},
doi = {https://doi.org/10.1016/B978-0-08-100733-4.00007-X},
url = {https://www.sciencedirect.com/science/article/pii/B978008100733400007X},
author = {Milan N. arevski and Vasko N. arevski},
keywords = {Two-stage centrifugal compressor, high impeller peripheral speed, high Mach number, low Reynolds number, large dimensions, high superheating, direct flash evaporation and condensation, high COP, high efficiency},
abstract = {R718 centrifugal refrigeration/heat pump systems are investigated to determine optimal schematic structure, to estimate performance characteristics, and to provide possibilities for wider application in the field of air-conditioning. Deep vacuum operating conditions, low specific volumetric cooling capacity, small molecular mass, and high isentropic exponent of the water are peculiarities which determine the main parameters of centrifugal compressors and the range of rational application of R718 refrigeration/heat pump systems. High impeller peripheral speed, high Mach number, low Reynolds number, large dimensions, and high superheating at the compressor discharge are the features of R718 centrifugal refrigeration compressors. A two-stage centrifugal compressor and, respectively, two-stage refrigeration cycle are optimal technical solutions for refrigeration systems in the field of air-conditioning applications. Direct flash evaporation and condensation provide simplicity and the possibility of achieving a high COP. A comparison shows that R718 centrifugal refrigeration systems are competitive with traditional refrigeration systems.}
}
